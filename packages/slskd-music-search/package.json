{
    "name": "@spotify-to-plex/slskd-music-search",
    "version": "1.0.0",
    "description": "Library to find tracks in Soulseek music library using SLSKD.",
    "keywords": [
        "soulseek",
        "slskd",
        "search",
        "music"
    ],
    "license": "MIT",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "default": "./dist/*.js"
        },
        "./*/*": {
            "types": "./dist/*/*.d.ts",
            "default": "./dist/*/*.js"
        },
        "./*/*/*": {
            "types": "./dist/*/*/*.d.ts",
            "default": "./dist/*/*/*.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "clean": "rm -rf dist",
        "type-check": "tsc --noEmit",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "@spotify-to-plex/music-search": "workspace:*",
        "@spotify-to-plex/shared-types": "workspace:*",
        "axios": "^1.11.0",
        "qs": "^6.14.0"
    },
    "devDependencies": {
        "@types/node": "^20.3.0",
        "@types/qs": "^6.14.0",
        "typescript": "^5.0.0"
    }
}
